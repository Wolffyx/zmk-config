&timers3 {
	st,prescaler = <10000>;
	status = "okay";

	pwm3: pwm {
		pinctrl-0 = <&tim3_ch2_pc7>;
		pinctrl-names = "default";
		status = "okay";
	};
};

&timers4 {
	st,prescaler = <10000>;
	status = "okay";

	pwm4: pwm {
		pinctrl-0 = <&tim4_ch2_pb7>;
		pinctrl-names = "default";
		status = "okay";
	};
};

